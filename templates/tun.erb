# This file managed by Puppet
cert = <%= @cert_real %>

setuid = root
setgid = root
pid = <%= @pid %>

debug = <%= @debug %>

output = <%= @output_r %>

timeoutidle = <%= @timeoutidle %> 
options = <%= @options %>

[<%= @name %>]
<% if @accept -%>
  accept=<%= @accept %>
<% end -%>
<% if @connect -%>
  connect=<%= @connect %>
<% end -%>
<% if @client -%>
  client=yes
<% else -%>
  client=no
<% end -%>
